
FACT: PROC ( X ) RETURNS (FIXED) ;

	DCL X FIXED;

	IF X = 1 THEN RETURN 1;
	ELSE RETURN X * FACT ( X - 1);

END FACT;

FIBO: PROC ( X ) RETURNS (FIXED) ;

	DCL X FIXED;

	IF X = 1 THEN RETURN 1; ELSE IF X = 2 THEN
		
		RETURN 1; ELSE RETURN FIBO (X - 1) + FIBO (X-2);

END FIBO;

Main: PROC;

	DCL (A,B) FIXED;

	A , B = 3;

	CALL PRINT_I(A);
	CALL PRINT_I(B);
	A = FACT(5);
	B = FIBO(6);
	CALL PRINT_I(A);
	CALL PRINT_I(B);

END Main;
